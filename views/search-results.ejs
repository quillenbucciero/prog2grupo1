<!-- Head -->
<%- include('./partials/head') %> 

<body>
	<!-- Header -->
	<%- include('./partials/headerNormal') %> 
	<!-- /Header -->

	<!-- Resultados -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Resultados de b√∫squeda para: Alfajores</h2>
			</div>
			<!-- Show the "ultimos" products -->
		</div>
	</div>


	<div class="col-12 col-sm-6 col-lg-3">
		<section class="product-box">
			<a href="/product">
				<figure class="product-box_image">
					<img src="<%= productos[1].imagen %> " alt="<% productos[1].nombreProducto %> ">
				</figure>
				<article class="product-box_data">
					<h2><%= productos[1].nombreProducto %> </h2>
					<p><%= productos[1].descripcion %></p>
					<p>Comentarios: 12</p>
				</article>
			</a>
		</section>
	</div>

	

	<!-- footer -->
	<%- include('./partials/footer') %> 
	<!-- /footer -->
</body>

</html>